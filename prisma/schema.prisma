generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Book {
  id Int @id @default(autoincrement())
  judul String @db.VarChar(255)
  penulis String @db.VarChar(100)
  harga String @db.VarChar(100)
  stok Int

  @@map("books")
}
